 <%- include('../../../../partials/header.ejs') %>


<section class="content invoice">
      <div class="block-header" style="margin-top:5rem">
        <div class="row">
          <div class="col-lg-7 col-md-6 col-sm-12">
            <h2>
            REITs History
            </h2>
          </div>
          <div class="col-lg-5 col-md-6 col-sm-12">
            <ul class="breadcrumb float-md-right">
              <li class="breadcrumb-item">
                <a href="dashboard"><i class="zmdi zmdi-home"></i> Novax-Assets</a>
              </li>
              <li class="breadcrumb-item">
                <a href="javascript:void(0);">Properties</a>
              </li>
              <li class="breadcrumb-item active">History</li>
            </ul>
          </div>
        </div>
      </div>
      <div class="container-fluid">
        <div class="row clearfix">
          <div class="col-lg-12">
            
            <div class="tab-content">
              <div
                role="tabpanel"
                class="tab-pane in active"
                id="details"
                aria-expanded="true"
              >
                <div class="card" id="details">
                  <div class="body">
                    <div class="row">
                      <div class="col-md-12">
                        <div class="table-responsive">
                          <table
                            id="mainTable"
                            class="table table-striped"
                            style="cursor: pointer"
                          >
                            <thead>
                              <tr>
                                <th>#</th>
                                <th>Amount</th>
                                <th>Property Name</th>
                                <th>Image</th>
                                <th>Description</th>
                                <th>Status</th>
                                <th>Date</th>
                              </tr>
                            </thead>
                                                        <tbody>
                                                           <!-- <tr class="clickrow" data-ref="31292"></tr> -->

                                                          <% events.forEach((event, index) => { %>
                                                            <tr class="clickrow" data-ref="<%= event.id %>">
                                                          
                                                              <td><%= event.id %></td>
                                                              <td>$<%= event.amount %></td>
                                                              <td><%= event.property_name %></td>
                                                              <td><img src="/uploads/reits/<%= event.image %>" alt="" srcset=""></td>
                                                              <td><p class="rendered-text md-item"><p><%= event.property_description %></td>
                                                              <td><span class="<%= event.status === 'completed' ? 'text-success' : 'text-warning' %>"><%= event.status %></span></td>
                                                              <td><%= event.date.toLocaleString() %></td>
                                                              <td><span class="btn btn-secondary"> View</span></td>
                                                            </tr>
                                                          <% }); %>

                                                   
                              
                            </tbody>
                             </table>
                        </div>
                      </div>
                    </div>
                   
                  
                  </div>
                </div>
              </div>
              
            
            </div>
          </div>
        </div>
      </div>
    </section>

    <script src="/assets/bundles/libscripts.bundle.js"></script>
    <script src="/assets/bundles/vendorscripts.bundle.js"></script>
    <script src="/assets/bundles/mainscripts.bundle.js"></script>
    <script>
    const elements = document.getElementsByClassName("clickrow");
    const giveReceipt = function() {
      const ref = this.getAttribute('data-ref');
      const URL = "payment?id=" + ref;
      openInNewTab(URL);
    };

    for (let i = 0; i < elements.length; i++) {
      elements[i].addEventListener('click', giveReceipt, false);
    }

    function openInNewTab(href) {
      Object.assign(document.createElement('a'), {
        target: '_blank',
        href: href,
      }).click();
    }
  </script>
  </body>

</html>